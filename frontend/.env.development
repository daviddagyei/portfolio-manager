REACT_APP_ITEMS_PER_PAGE=50
REACT_APP_MAX_ITEMS_PER_PAGE=200

# External Services
REACT_APP_ENABLE_YAHOO_FINANCE=true
REACT_APP_ENABLE_ALPHA_VANTAGE=false
